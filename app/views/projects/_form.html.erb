<%= simple_form_for(@project, method: :put) do |f| %>

  <%= f.input :name %>
  <%= f.input :caption %>
  <%= f.input :content %>
  <%= f.input :github %>
  <%= f.input :url %>
  <%= f.input :blog %>
  <%= f.input :image, as: :file %>
  <%= f.input :skills, input_html: { name: "project[skills]" } %>
  <%= f.submit %>

<% end %>
