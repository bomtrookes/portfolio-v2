<%= simple_form_for(@project) do |f| %>

  <%= f.input :cover_image, as: :file %>
  <%= f.input :color_theme %>
  <%= f.input :name %>
  <%= f.input :caption %>
  <%= f.input :content %>
  <%= f.input :github %>
  <%= f.input :url %>
  <%= f.input :blog %>
  <%= f.input :images, as: :file, input_html: { multiple: true } %>
  <%= f.input :skills, input_html: { name: "project[skills]" } %>
  <%= f.submit %>

<% end %>
